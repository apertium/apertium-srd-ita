  <!-- apoderare > appropriare, insediare -->
  <rule weight="0.8">
    <match lemma="apoderare" tags="vblex.*"><select lemma="appropriare"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="apoderare" tags="vblex.*"><select lemma="insediare"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="in" tags="pr"/>
  </rule>
  <rule weight="1.0">
    <match lemma="nche" tags="prn.pro.*"/>
    <match lemma="apoderare" tags="vblex.*"><select lemma="insediare"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="nche" tags="prn.pro.*"/>
    <or>
      <match tags="vbser.*"/>
      <match tags="vbhaver.*"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="apoderare" tags="vblex.*"><select lemma="insediare"/></match>
  </rule>

  <!-- refudare > rifiutare, ... -->
  <rule weight="0.8">
    <match lemma="refudare" tags="vblex.*"><select lemma="rifiutare"/></match>
  </rule>

  <!-- crèschere > crescere, accrescere -->
  <macro n="verb_intrans_trans"><with-param v="crèschere"/><with-param v="crescere"/><with-param v="accrescere"/></macro>

  <!-- girare > girare, ... -->
  <rule weight="0.8">
    <match lemma="girare" tags="vblex.*"><select lemma="girare"/></match>
  </rule>

  <!-- giùghere > portare, avere, ... -->
  <rule weight="0.8">
    <match lemma="giùghere" tags="vblex.*"><select lemma="portare"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="giùghere" tags="vblex.*"><select lemma="avere"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <or>
      <seq n="corpus"/>
    </or>
  </rule>
  <rule weight="1.2">
    <match lemma="giùghere" tags="vblex.*"><select lemma="portare"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <or>
      <seq n="corpus"/>
    </or>
    <match tags="pr"/>
  </rule>

  <!-- iscrìere > scrivere, iscrivere -->
  <rule weight="0.8">
    <match lemma="iscrìere" tags="vblex.*"><select lemma="scrivere"/></match>
  </rule>

  <!-- disconnòschere > ignorare, disconoscere, ... -->
  <rule weight="0.8">
    <match lemma="disconnòschere" tags="vblex.*"><select lemma="ignorare"/></match>
  </rule>

  <!-- cumpònnere > comporre, digitare -->
  <rule weight="0.8">
    <match lemma="cumpònnere" tags="vblex.*"><select lemma="comporre"/></match>
  </rule>

  <!-- pònnere > mettere, .... -->
  <rule weight="0.8">
    <match lemma="pònnere" tags="vblex.*"><select lemma="mettere"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="pònnere" tags="vblex.*"><select lemma="porre"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*"/>
    <or>
      <match lemma="atzentu" tags="n.*"/>
      <match lemma="base" tags="n.*"/>
    </or>
  </rule>

  <!-- pàrrere > sembrare, parrere -->
  <rule weight="0.8">
    <match lemma="pàrrere" tags="vblex.*"><select lemma="sembrare"/></match>
  </rule>

