    <!-- Aquesta macro tria el verb p2 per traduir el prenom p1, però manté p1 si va seguit de cognom (o paraula desconeguda) -->
    <def-macro n="ant_antcog" npar="2">
      <rule weight="0.6">
         <match plemma="1" tags="np.ant.*"><select plemma="2"/></match>
      </rule>
      <rule weight="1.0">
         <match plemma="1" tags="np.ant.*"><select plemma="1"/></match>
         <or>
           <match tags="np.cog.*"/>
           <match tags="" case="Aa"/>
         </or>
      </rule>
      <rule weight="1.0">
         <or>
           <match lemma="sennore" tags="n.*"/>
           <match lemma="segnore" tags="n.*"/>
           <match lemma="sennorinu" tags="n.*"/>
         </or>
         <match plemma="1" tags="np.*"><select plemma="1"/></match>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria el verb p2 si p1 és intransitiu i p3 si p1 és transitiu -->
    <!-- p1 = verb fra, p2 = verb cat intransitiu, p3 = verb cat transitiu -->
    <!-- la manera de veure que el verb és transitiu és molt barroera -->
    <def-macro n="verb_intrans_trans" npar="3">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <repeat from="0" upto="1">
          <match tags="predet.*"/>
        </repeat>
        <match tags="det.*"/>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="np.*"/>
          <match tags="" case="Aa"/>
        </or>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <repeat from="0" upto="1">
          <match tags="det.*"/>
        </repeat>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="1">
          <or>
            <match tags="adv"/>
            <match tags="preadv"/>
          </or>
        </repeat>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0">
        <match lemma="lu" tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <match lemma="lu" tags="prn.enc.*"/>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="vblex.pp.*"/>
        </or>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <repeat from="0" upto="1">
          <match lemma="a" tags="pr"/>
        </repeat>
        <match tags="*.inf"/>
      </rule>

      <rule weight="1.2">
        <match lemma="fare" tags="vblex."/>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>
        <match plemma="1" tags="vblex.inf"><select plemma="2"/></match>
      </rule>

      <rule weight="1.2">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>

        <repeat from="0" upto="1"> <!-- data -->
          <match tags="det.*"/>
        </repeat>
        <repeat from="0" upto="1">
          <seq n="die"/>
        </repeat>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
        <seq n="mese"/>
        <repeat from="0" upto="1">
          <match tags="num"/>
        </repeat>

        <or>
          <match tags="sent"/>
          <match tags="cm"/>
          <match tags="cnjcoo"/>
          <match tags="cnjadv"/>
        </or>
      </rule>

      <rule weight="1.2">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
        <repeat from="0" upto="1">
          <match tags="adv"/>
        </repeat>

        <repeat from="0" upto="1"> <!-- data -->
          <match tags="predet.*"/>
        </repeat>
        <repeat from="0" upto="1">
          <match tags="det.*"/>
        </repeat>
        <seq n="die"/>
        <repeat from="0" upto="1">
          <or>
            <match tags="num.*"/>
            <match tags="num"/>
          </or>
        </repeat>
        <repeat from="0" upto="1">
          <seq n="mese"/>
        </repeat>
        <repeat from="0" upto="1">
          <match tags="num"/>
        </repeat>

        <or>
          <match tags="sent"/>
          <match tags="cm"/>
          <match tags="cnjcoo"/>
          <match tags="cnjadv"/>
        </or>
      </rule>
