  <section-rules>
    <rule comment="RÈGLA: SN SV_copulatiu SA: sa die est bella > lo giorno è bello">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="SV_copulatiu"/>
        <pattern-item n="SA"/>
      </pattern>
      <action>
        <call-macro n="f_acord_subj_vcop_atrib3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="whole"/>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RÈGLA: SN SV_copulatiu SAdv SA: sa die est semper bella > lo giorno è sempre bello">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="SV_copulatiu"/>
        <pattern-item n="SAdv"/>
        <pattern-item n="SA"/>
      </pattern>
      <action>
        <call-macro n="f_acord_subj_vcop_atrib3"><with-param pos="1"/><with-param pos="2"/><with-param pos="4"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="whole"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="whole"/>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RÈGLA: SN PREP SN SV_copulatiu SA: sas dies de Pietro sunt bellas > i giorni di Petro sono belli">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="PREP"/>
        <pattern-item n="SN"/>
        <pattern-item n="SV_copulatiu"/>
        <pattern-item n="SA"/>
      </pattern>
      <action>
        <call-macro n="f_acord_subj_vcop_atrib3"><with-param pos="1"/><with-param pos="4"/><with-param pos="5"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="whole"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="whole"/>
          </chunk>
          <b pos="4"/>
          <chunk>
            <clip pos="5" part="whole"/>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RÈGLA: SN PREP SN SV_copulatiu SAdv SA: sas dies de Pietro sunt semper bellas > i giorni di Petro sono sempre belli">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="PREP"/>
        <pattern-item n="SN"/>
        <pattern-item n="SV_copulatiu"/>
        <pattern-item n="SAdv"/>
        <pattern-item n="SA"/>
