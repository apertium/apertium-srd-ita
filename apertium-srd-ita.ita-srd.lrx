<!-- **************** PREP **************** -->

  <!-- a > a, in -->
  <rule weight="0.8">
    <match lemma="a" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in"/></match>
    <match tags="np.top.*"/>
    <repeat from="0" upto="1">
