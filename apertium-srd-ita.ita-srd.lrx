      <match lemma="Apuleio" tags="np.*"/>
      <match lemma="Settimio" tags="np.*"/>
    </or>
  </rule>

  <!-- Mario > Mario, Màriu -->
  <rule weight="0.6">
    <match lemma="Mario" tags="np.ant.*"><select lemma="Mario"/></match>
  </rule>
  <rule weight="2.0">
    <match lemma="Mario" tags="np.ant.*"><select lemma="Mario"/></match>
    <match tags="np.cog.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="Mario" tags="np.ant.*"><select lemma="Màriu"/></match>
    <repeat from="0" upto="8">
      <or>
        <match tags="*"/>
        <match tags=""/>
      </or>
    </repeat>
    <or>
      <match lemma="Silla" tags="np.*"/>
      <match lemma="Roma" tags="np.*"/>
      <match lemma="console" tags="n.*"/>
      <match lemma="romano" tags="n.*"/>
      <match lemma="romano" tags="adj.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="Silla" tags="np.*"/>
      <match lemma="Roma" tags="np.*"/>
      <match lemma="console" tags="n.*"/>
      <match lemma="romano" tags="n.*"/>
      <match lemma="romano" tags="adj.*"/>
    </or>
    <repeat from="0" upto="8">
      <or>
        <match tags="*"/>
        <match tags=""/>
      </or>
    </repeat>
    <match lemma="Mario" tags="np.ant.*"><select lemma="Màriu"/></match>
  </rule>

  <!-- Asburgo > Asburgu, Asburgos -->
  <rule weight="0.6">
    <match lemma="Asburgo" tags="np.cog.*"><select lemma="Asburgu"/></match>
  </rule>
  <rule weight="1.0">
