<!-- **************** NP **************** -->

  <!-- Arborea > Arborea, Arboree -->
  <rule weight="0.6">
    <match lemma="Arborea" tags="np.top.*"><select lemma="Arborea"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="giudicato" tags="n.*"/>
      <match lemma="regno" tags="n.*"/>
      <match lemma="giudice" tags="n.*"/>
      <match lemma="re" tags="n.*"/>
    </or>
    <match lemma="di" tags="pr"/>
    <match lemma="Arborea" tags="np.top.*"><select lemma="Arbaree"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match tags="np.ant.*"/>
      <match tags="" case="Aa"/>
    </or>
    <repeat from="0" upto="1">
      <or>
        <match tags="num.*"/>
        <match lemma="I" tags="adj.*"/>
        <match lemma="II" tags="adj.*"/>
        <match lemma="III" tags="adj.*"/>
        <match lemma="IV" tags="adj.*"/>
        <match lemma="V" tags="adj.*"/>
        <match lemma="VI" tags="adj.*"/>
        <match lemma="VII" tags="adj.*"/>
        <match lemma="VIII" tags="adj.*"/>
        <match lemma="IX" tags="adj.*"/>
        <match lemma="X" tags="adj.*"/>
      </or>
    </repeat>
    <match lemma="di" tags="pr"/>
    <match lemma="Arborea" tags="np.top.*"><select lemma="Arbaree"/></match>
  </rule>

