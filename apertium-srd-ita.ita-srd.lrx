  <!-- Lucio > Lucio, Lùtziu -->
  <rule weight="0.6">
    <match lemma="Lucio" tags="np.ant.*"><select lemma="Lucio"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="Lucio" tags="np.ant.*"><select lemma="Lùtziu"/></match>
