    </repeat>
    <match lemma="ci" tags="prn.pro.adv"/>
    <match tags="vbser.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in"/></match>
    <or>
      <match lemma="San" tags="np.*"/>
      <match lemma="Santa" tags="np.*"/>
    </or>
    <or>
      <match tags="np.*"/>
      <match tags=""/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="ci" tags="prn.pro.adv"/>
    <match tags="vbser.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in"/></match>
    <or>
      <seq n="mese"/>
      <match lemma="tavola" tags="n.*"/>
