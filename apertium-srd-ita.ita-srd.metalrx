    <match lemma="di" tags="pr"/>
    <match tags="det.*"/>
    <or>
      <match lemma="lavoratore" tags="n.*"/>
      <match lemma="sindacato" tags="n.*"/>
      <match lemma="rappresentante" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="aumento" tags="n.*"/>
      <match lemma="incremento" tags="n.*"/>
      <match lemma="diminuzione" tags="n.*"/>
      <match lemma="sostegno" tags="n.*"/>
    </or>
    <match lemma="di" tags="pr"/>
    <match tags="det.def.*"/>
    <match lemma="domanda" tags="n.*"><select lemma="dimanda"/></match>
  </rule>

  <!-- tassa > imposta... -->
  <rule weight="0.8">
    <match lemma="tassa" tags="n.*"><select lemma="imposta"/></match>
  </rule>

  <!-- papà > babbu, ba' -->
  <rule weight="0.8">
    <match lemma="papà" tags="n.*"><select lemma="babbu"/></match>
  </rule>

  <!-- divisione > partzidura, divisione -->
  <rule weight="0.6">
     <match lemma="divisione" tags="n.*"><select lemma="partzidura"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="generale" tags="n.*"/>
      <match lemma="ammiraglio" tags="n.*"/>
      <match lemma="esercito" tags="n.*"/>
      <match lemma="battaglia" tags="n.*"/>
      <match lemma="guerra" tags="n.*"/>
      <match lemma="fanteria" tags="n.*"/>
      <match lemma="cabelleria" tags="n.*"/>
      <match lemma="artiglieria" tags="n.*"/>
      <match lemma="blindato"/>
    </or>
    <repeat from="0" upto="8">
      <match/>
    </repeat>
    <match lemma="divisione" tags="n.*"><select lemma="divisione"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="divisione" tags="n.*"><select lemma="divisione"/></match>
    <repeat from="0" upto="8">
      <match/>
    </repeat>
    <or>
      <match lemma="generale" tags="n.*"/>
      <match lemma="ammiraglio" tags="n.*"/>
      <match lemma="esercito" tags="n.*"/>
      <match lemma="battaglia" tags="n.*"/>
      <match lemma="guerra" tags="n.*"/>
      <match lemma="fanteria" tags="n.*"/>
      <match lemma="cabelleria" tags="n.*"/>
      <match lemma="artiglieria" tags="n.*"/>
      <match lemma="blindato"/>
    </or>
  </rule>

  <!-- vita > bida, vida (dcorraine), chintu -->
  <rule weight="0.6">
    <match lemma="vita" tags="n.*"><select lemma="vida"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="da" tags="pr"/>
      <match lemma="fino a" tags="pr"/>
    </or>
    <match tags="det.def.*"/>
    <match lemma="vita" tags="n.*"><select lemma="chintu"/></match>
  </rule>
  <rule weight="1.2">
    <or>
      <match lemma="da" tags="pr"/>
      <match lemma="fino a" tags="pr"/>
    </or>
    <match tags="det.def.*"/>
    <match lemma="vita" tags="n.*"><select lemma="vida"/></match>
    <match lemma="di" tags="pr"/>
  </rule>
  <rule weight="1.0">
    <match lemma="vita" tags="n.*"><select lemma="chintu"/></match>
    <repeat from="0" upto="1">
      <or><match tags="adv"/><match tags="preadv"/></or>
    </repeat>
    <or>
      <match lemma="stretto" tags="adj.*"/>
      <match lemma="alto" tags="adj.*"/>
      <match lemma="basso" tags="adj.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="vita" tags="n.*"><select lemma="chintu"/></match>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="anca" tags="n.*"/>
      <match lemma="pancia" tags="n.*"/>
      <match lemma="ventre" tags="n.*"/>
      <match lemma="petto" tags="n.*"/>
      <match lemma="seno" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="anca" tags="n.*"/>
      <match lemma="pancia" tags="n.*"/>
      <match lemma="ventre" tags="n.*"/>
      <match lemma="petto" tags="n.*"/>
      <match lemma="seno" tags="n.*"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <match lemma="vita" tags="n.*"><select lemma="chintu"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="vita" tags="n.*"><select lemma="vida"/></match>
    <match lemma="sociale" tags="adj.*"/>
  </rule>

  <!-- donna > fèmina, mugere -->
  <rule weight="0.6">
    <match lemma="donna" tags="n.*"><select lemma="fèmina"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="adj.pos.*"/>
    <repeat from="0" upto="1">
      <match tags="adj.*"/>
    </repeat>
    <match lemma="donna" tags="n.*"><select lemma="mugere"/></match>
  </rule>

  <!-- nipote > nepode, neta -->
  <rule weight="0.6">
    <match lemma="nipote" tags="n.*"><select lemma="nepode"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="*.f.*"/>
    <match lemma="nipote" tags="n.*"><select lemma="neta"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="nipote" tags="n.*"><select lemma="neta"/></match>
    <repeat from="0" upto="1">
      <or><match tags="adv"/><match tags="preadv"/></or>
    </repeat>
    <match tags="*.f.*"/>
  </rule>

  <!-- pronipote > nepode segundu, grandu -->
  <rule weight="0.6">
    <match lemma="pronipote" tags="n.*"><select lemma="nepode segundu"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="*.f.*"/>
    <match lemma="pronipote" tags="n.*"><select lemma="neta segunda"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="pronipote" tags="n.*"><select lemma="neta segunda"/></match>
    <match tags="*.f.*"/>
  </rule>

<!-- **************** ADJ **************** -->

  <!-- casalingo > de domo, in domo -->
  <rule weight="0.8">
    <match lemma="casalingo" tags="adj.*"><select lemma="de domo"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="partita" tags="n.*"/>
      <match lemma="edizione" tags="n.*"/>
      <match lemma="campionato" tags="n.*"/>
      <match lemma="premio" tags="n.*"/>
      <match lemma="gara" tags="n.*"/>
      <match lemma="vittoria" tags="n.*"/>
      <match lemma="sconfitta" tags="n.*"/>
    </or>
    <match lemma="casalingo" tags="adj.*"><select lemma="in domo"/></match>
  </rule>

  <!-- special > ispetziale, special -->
  <rule weight="0.8">
    <match lemma="special" tags="adj.*"><select lemma="ispetziale"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="Special" tags="adj.*"><select lemma="special"/></match>
  </rule>

  <!-- azzurro > asulu, baiatu, blau -->
  <rule weight="0.8">
    <match lemma="azzurro"><select lemma="asulu"/></match>
  </rule>

  <!-- reale > beru, reale -->
  <rule weight="0.5">
    <match lemma="reale" tags="adj.*"><select lemma="beru"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="altezza" tags="n.*"/>
      <match lemma="casa" tags="n.*"/>
      <match lemma="villa" tags="n.*"/>
      <match lemma="corona" tags="n.*"/>
      <match lemma="famiglia" tags="n.*"/>
      <match lemma="accademia" tags="n.*"/>
      <match lemma="monastero" tags="n.*"/>
      <match lemma="via" tags="n.*"/>
    </or>
    <match lemma="reale" tags="adj.*"><select lemma="reale"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="reale" tags="adj.*"><select lemma="reale"/></match>
    <or>
      <match lemma="accademia" tags="n.*"/>
      <match lemma="monastero" tags="n.*"/>
    </or>
  </rule>

  <!-- caro > caru, istimadu -->
  <rule weight="0.6">
    <match lemma="caro" tags="adj.*"><select lemma="caru"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="caro" tags="adj.*"><select lemma="istimadu"/></match>
    <or>
      <seq n="famiglia"/>
      <match lemma="amico" tags="n.*"/>
      <match tags="np.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="famiglia"/>
      <match lemma="amico" tags="n.*"/>
      <match tags="np.*"/>
    </or>
    <match lemma="caro" tags="adj.*"><select lemma="istimadu"/></match>
  </rule>

  <!-- gran > mannu, grandu -->
  <rule weight="0.6">
     <match lemma="grande" tags="adj.*"><select lemma="mannu"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="grande" tags="adj.*"><select lemma="grandu"/></match>
     <match tags="n.*"/>
  </rule>


<!-- **************** NP **************** -->

  <rule weight="0.6">
     <match lemma="Cornelio" tags="np.ant.*"><select lemma="Cornelio"/></match>
