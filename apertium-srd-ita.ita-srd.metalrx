    <match lemma="da" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>
  <rule weight="1.2">
    <or>
      <seq n="verbi_da"/>
      <match lemma="venire" tags="vblex.*"/>
      <match lemma="provenire" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat> 
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.2">
    <or>
      <match lemma="tornare" tags="vblex.*"/>
      <match lemma="ritornare" tags="vblex.*"/>
    </or>
    <match lemma="a" tags="pr"/>
    <match tags="*.inf"/>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat> 
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>
  <rule weight="1.4">
    <or>
      <match lemma="tornare" tags="vblex.*"/>
      <match lemma="ritornare" tags="vblex.*"/>
    </or>
    <match lemma="a" tags="pr"/>
    <or>
      <seq n="verbi_da"/>
