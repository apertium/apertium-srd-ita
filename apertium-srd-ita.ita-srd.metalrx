    </repeat>
    <match lemma="a" tags="pr"><select lemma="in"/></match>
  </rule>

  <!-- in > in, a, da -->
  <rule weight="0.8">
    <match lemma="in" tags="pr"><select lemma="in"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="andare" tags="vblex.*"/>
      <match lemma="arrivare" tags="vblex.*"/>
      <match lemma="partire" tags="vblex.*"/>
      <match lemma="ritornare" tags="vblex.*"/>
      <match lemma="venire" tags="vblex.*"/>
      <seq n="verbi_moto_lista_corta"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="in" tags="pr"><select lemma="a"/></match>
<!--
    <or>
      <match tags="det.*"/>
      <match tags="np.*"/>
    </or>
-->
  </rule>
  <rule weight="1.2">
    <match lemma="in" tags="pr"><select lemma="in"/></match>
    <or>
      <seq n="mezzi_di_trasporto"/>
      <!-- En principio, "a" se pone solo cuándo se hable de un lugar físico y concreto (p.e. rutu a terra, rutu a sa piscina) y en los otros casos, donde se trata de substantivos abstractos poner "in" (rutu in tentatzione, rutu in misèria etc) -->
      <match lemma="anticipo" tags="n.*"/>
