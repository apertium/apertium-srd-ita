  <!-- su > subra de, subra, in -->
  <rule weight="0.8">
     <match lemma="su" tags="pr"><select lemma="subra de"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="su" tags="pr"><select lemma="in"/></match>
    <match tags="det.*"/>
    <match lemma="pagina" tags="n.*"/>
