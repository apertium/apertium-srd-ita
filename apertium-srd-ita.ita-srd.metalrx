      <match lemma="dire" tags="vblex.*"/>
      <match lemma="soprannominare" tags="vblex.*"/>
      <match lemma="eleggere" tags="vblex.*"/>
      <match lemma="nominare" tags="vblex.*"/>
      <match lemma="proclamare" tags="vblex.*"/>
