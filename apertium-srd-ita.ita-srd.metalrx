<!-- **************** DET **************** -->

  <!-- il > su -->
  <rule weight="0.8">
    <match lemma="il" tags="det.*"><select lemma="su"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="giocare" tags="vblex.*"/>
    <match lemma="a" tags="pr"/>
    <match lemma="il" tags="det.*"><remove tags="det.*"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="giocare" tags="vblex.*"/>
    <match lemma="a" tags="pr"/>
    <match lemma="il" tags="det.*"><select lemma="su"/></match>
    <or>
      <match lemma="stadio" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="terreno" tags="n.*"/>
      <match lemma="parco" tags="n.*"/>
      <match lemma="giardino" tags="n.*"/>
    </or>
  </rule>

<!-- **************** REL **************** -->

  <!-- come > comente a, comente -->
  <rule weight="0.8">
    <match lemma="come" tags="rel.*"><select lemma="comente a"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="vblex.pp.*"/>
    <repeat from="0" upto="1">
