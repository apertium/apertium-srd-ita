  <!-- per > pro, a -->
  <rule weight="0.6">
    <match lemma="per" tags="pr"><select lemma="pro"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cadere" tags="vblex.*"/>
    <match lemma="per" tags="pr"><select lemma="a" tags="pr"/></match>
    <match lemma="terra" tags="n.*"/>
  </rule>

  <!-- da > de, dae, a -->
  <rule weight="0.6">
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>
  <rule weight="1.0"> <!-- complemento agente introducido por "dae" en sardo -->
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <match lemma="da" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="verbi_da"/>
      <match lemma="venire" tags="vblex.*"/>
      <match lemma="provenire" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <match lemma="da" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>
  <rule weight="1.2">
    <or>
      <seq n="verbi_da"/>
      <match lemma="venire" tags="vblex.*"/>
      <match lemma="provenire" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat> 
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.2">
    <or>
      <match lemma="tornare" tags="vblex.*"/>
      <match lemma="ritornare" tags="vblex.*"/>
    </or>
    <match lemma="a" tags="pr"/>
    <match tags="*.inf"/>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat> 
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>
  <rule weight="1.4">
    <or>
      <match lemma="tornare" tags="vblex.*"/>
      <match lemma="ritornare" tags="vblex.*"/>
    </or>
    <match lemma="a" tags="pr"/>
    <or>
      <seq n="verbi_da"/>
      <match lemma="venire" tags="vblex.inf"/>
      <match lemma="provenire" tags="vblex.inf"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat> 
    <match lemma="da" tags="pr"><select lemma="dae" tags="pr"/></match>
  </rule>

<!--
  <rule weight="1.0">
    <match lemma="essere" tags="vbser.*"/>
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
