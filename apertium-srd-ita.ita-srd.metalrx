</rule> 



<!-- economico > econòmicu, baratu -->
<rule weight="0.8"> 
    <match lemma="economico" tags="adj.*"><select lemma="econòmicu"/></match>
</rule> 
  <rule weight="1.0">
  <!-- "molto economico →  meda baratu -->
    <or>
       <match lemma="molto" tags="adv"/>   <!-- just in case the tagger didn't get it right -->
       <match lemma="molto" tags="preadv"/>
    </or>
    <match lemma="economico" tags="adj.*"><select lemma="baratu"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="vbser"/>
    <repeat from="0" upto="1">
      <or><match tags="adv"/><match tags="preadv"/></or>
    </repeat>
    <match lemma="economico" tags="adj.*"><select lemma="baratu"/></match>
  </rule>


  <!-- vite > toronìlliu, vite  -->
  <rule weight="0.8"> 
    <match lemma="vite" tags="n.*"><select lemma="toronìlliu"/></match>
  </rule> 
  <rule weight="1.0">
    <or>
      <match lemma="piantare" tags="vblex.*"/>
      <match lemma="coltivare" tags="vblex.*"/>
      <match lemma="cogliere" tags="vblex.*"/>
      <match lemma="vendemmiare" tags="vblex.*"/>
      <match lemma="vendemmia" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="uva" tags="n.*"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <match lemma="vite" tags="n.*"><select lemma="vite"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="vite" tags="n.*"><select lemma="vite"/></match>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="piantare" tags="vblex.*"/>
      <match lemma="coltivare" tags="vblex.*"/>
      <match lemma="cogliere" tags="vblex.*"/>
      <match lemma="vendemmiare" tags="vblex.*"/>
      <match lemma="vendemmia" tags="n.*"/>
      <match lemma="campo" tags="n.*"/>
      <match lemma="uva" tags="n.*"/>
    </or>
  </rule>


<!--CORSO n.m > CORSICANU/CURSU -->

  <rule weight="0.8">
    <match lemma="corso" tags="n.m.sg"><select lemma="cursu" tags="n.m.sg"/></match>
  </rule>

  <rule weight="0.8">
    <match lemma="corso" tags="n.m.pl"><select lemma="corsicanu" tags="n.m.pl"/></match>
